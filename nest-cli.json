{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/app",
  "projects": {
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "data-fetcher": {
      "type": "application",
      "root": "apps/data-fetcher",
      "entryFile": "main",
      "sourceRoot": "apps/data-fetcher/src",
      "compilerOptions": {
        "tsConfigPath": "apps/data-fetcher/tsconfig.app.json"
      }
    },
    "lp-bot": {
      "type": "application",
      "root": "apps/lp-bot",
      "entryFile": "main",
      "sourceRoot": "apps/lp-bot/src",
      "compilerOptions": {
        "tsConfigPath": "apps/lp-bot/tsconfig.app.json"
      }
    },
    "test-fn": {
      "type": "application",
      "root": "apps/test-fn",
      "entryFile": "main",
      "sourceRoot": "apps/test-fn/src",
      "compilerOptions": {
        "tsConfigPath": "apps/test-fn/tsconfig.app.json"
      }
    },
    "core": {
      "type": "application",
      "root": "apps/core",
      "entryFile": "main",
      "sourceRoot": "apps/core/src",
      "compilerOptions": {
        "tsConfigPath": "apps/core/tsconfig.app.json"
      }
    }
  }
}